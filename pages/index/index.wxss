  color: #fff;
  border-radius: 3px;
  display: inline-block;
  width: 16px;
  height: 15px;
  line-height: 17px;
  text-align: center;
  font-size: 12px;
  margin: 0 2px;
}

.group-list-box .discount-list .day{
  font-size: 12px;
}

.group-list-box .discount-list .ul .li .goods_info .goods_name {
  color: #666;
  margin: 5px 0 0 5px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.group-list-box .discount-list .ul .li .goods_info .goods_price {
  background: #f44;
  color: #fff;
  border-radius: 3px;
  margin: 5px 0 0 5px;
  padding: 1px 3px;
  display: inline-block;
  font-size: 13px;
}

.group-list-box .discount-list .ul .li .goods_info .goods_price .i {
  font-size: 12px;
  font-style: normal;
}

.group-list-box .discount-list .ul .li .goods_pic{
  display: block;
  width: 35%;
  position: relative;
  float: left;
  height: 110px;
}

.group-list-box .discount-list .ul .li .goods_pic .a {
  display: block;
  width: 96%;
  position: absolute;
  height: 100px;
  top: 50%;
  transform: translateY(-50%);
  line-height: 110px;
}

.group-list-box .discount-list .ul .li .goods_pic .a image {
  max-width: 100%;
  max-height: 100%;
  vertical-align: middle;
  margin-bottom: 20px;
}


