}

.discount-title image{
  width: 15px;
  height: 15px;
  margin-bottom:3px;
  vertical-align:middle;
}

.group-list-box .discount-list .ul .li .goods_info .remaining_time {
  margin: 5px 0 0 5px;
  font-size: 12px;
}

.group-list-box .discount-list .ul .li .goods_info .remaining_time .span.hours, .group-list-box .discount-list .ul .li .goods_info .remaining_time .span.min, .group-list-box .discount-list .ul .li .goods_info .remaining_time .span.seconds {
