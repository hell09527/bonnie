/* 商品分类标题 */
.goods-detail-nav{
    padding:18rpx 20rpx;
    background: #fff;
    white-space:nowrap;
    box-sizing: border-box;
    /* box-shadow: 0px 0px 20px rgba(0,0,0,0.1); */
    border-bottom: 1px solid #e5e5e5;
  }
  .fixed{
    position: fixed;
    top:0;
    z-index: 10;
    left: 0;
  }
  .fixed-detail{
    margin-top:90rpx;
  }
  .nav-item{
    display: inline-block;
    font-size:14px;
    color:#808080;
    border:1px solid transparent;
    padding:0 30rpx;
    line-height: 48rpx;
    text-align: center;
  }
  .nav-item-active{
    border:1px solid #000;
    border-radius: 50rpx;
    color:#000;
    font-weight: 500;
  }
  
.body{
  /* padding:20rpx 0 0; */
  box-sizing: border-box;
  /* display: table; */
    position: fixed;
  width:100%;
  height:720rpx;
padding-bottom:50px;

}

  .goods_all{
    width:33.3%;
    float: left;
    box-sizing: border-box;
  }
  .goods_all:nth-child(2n)>.all-box{
    margin:0 20rpx 20rpx 10rpx;
  }
  .all-box{
    position: relative;
    margin:0 10rpx 20rpx 20rpx;
    background: #fff;
    box-sizing: border-box;
  }
  .all-box>image{
    width:100%;
    display: block;
  }
  .shop-detail{
    padding:10px;
  }
  .title{
    font-size: 12px;
    line-height: 18px;
    height:100rpx;
    display: -webkit-box; 
    text-overflow: ellipsis;
  -webkit-line-clamp: 2; 
  -webkit-box-orient: vertical; 
  overflow: hidden;
  }
  
  .shop-detail-price{
    display: flex;
    justify-content: flex-start;
    padding:10rpx 0 0;
    align-items: center;
  }
  .shop-detail-price>text{
    color:#000;
    margin-right: 10px;
    font-weight:700;
  }
  .i.goods_tab {
    font-style: normal;
    font-size: 12px !important;
    background: #333;
    color: #fff !important;
    padding: 1px 3px;
    margin-right: 2px;
    border-radius: 2px;
    font-weight: normal !important;
  }
  .selected{
    position: absolute;
    right:0;
    top:0;
    width:40rpx;
    height:40rpx;
  }
  .roll{
    width: 100%;
  }