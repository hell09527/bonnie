/* pages/member/resgin/resgin.wxss */
page{
  background: #fff;
}
.containBox{
  position: fixed;
  top:0;
  left:0;
  height:100%;
  width:100%;
}
.box{
  position: absolute;
  left:50%;
  top:50%;
  transform: translateX(-50%) translateY(-50%);
}
.box>image{
  display: block;
  width:200rpx;
  height:200rpx;
  margin:0 auto 60rpx;
}
.wechatLogin{
  background: #000;
  color:#fff;
  text-align: center;
  height:100rpx;
  width:700rpx;
  line-height: 100rpx;
  font-size: 14px;
  border:1px solid #000;
  margin-bottom: 40rpx;
}
.phoneLogin{
  background: #fff;
  border:1px solid #000;
  color:#000;
  text-align: center;
  height:100rpx;
  width:700rpx;
  line-height: 100rpx;
  font-size: 14px;
  margin-bottom: 40rpx;
}
button{
  border-radius: 0;
}
