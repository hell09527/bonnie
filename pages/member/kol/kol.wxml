      <!-- 商品推荐排行榜 -->
      <block wx:for="{{listData}}" wx:key="{{code}}"wx:for-item='item' wx:for-index='index' wx:if="{{isTopClick==1}}">
        <view class="tr" catchtap='skipClick' data-url='/goods/goodsdetail/goodsdetail?goods_id={{item.goods_id}}&&goods_name={{item.goods_name}}'>
          <!-- 第一 -->
          <view class="sequence" style='color:#d0021b' wx:if='{{index==0}}'>{{index+1}}</view>
          <!-- 第二 -->
          <view class="sequence" style='color:#f57923' wx:elif='{{index==1}}'>{{index+1}}</view>
          <!-- 第三 -->
          <view class="sequence" style='color:#f9b649' wx:elif='{{index==2}}'>{{index+1}}</view>
          <!-- 其他 -->
          <view class="sequence" wx:else>{{index+1}}</view>
          <!-- <view class="brand">{{item.text}}</view> -->
          <view class="names">
          <image class="img" src='{{item.pic_cover_small}}'></image>
          <view class="w_book">{{item.goods_name}}</view>
          </view>
          <!-- 第一 -->
          <view class="ratio" style='color:#d0021b' wx:if='{{index==0}}'>
            <text>分润</text>
            <text>{{item.fraction}}</text>
          </view>
          <!-- 第二 -->
          <view class="ratio" style='color:#f57923' wx:elif='{{index==1}}'>
            <text>分润</text>
            <text>{{item.fraction}}</text>
          </view>
          <!-- 第三 -->
          <view class="ratio" style='color:#f9b649' wx:elif='{{index==2}}'>
            <text>分润</text>
            <text>{{item.fraction}}</text>
          </view>
          <!-- 其他 -->
          <view class="ratio" wx:else>
            <text>分润</text>
            <text>{{item.fraction}}</text>
          </view>
        </view>
      </block>


      <!-- 商品销售排行榜 -->

