  top: 6px;
}
.list{
  width: 10px;
  height: 10px;
  background: #000;
  border: 1px solid #000;
  border-radius: 50%;
  font-size: 12px;
  line-height: 16px;
  text-align: center;
  color: #fff;
  position: absolute;
  z-index: 10;
  left:111rpx;
  top: 2px;
}

.yes {
  color: yellow;
  border: 1px solid yellow;
}

.member_list .ul {
  width: 100%;
  background-color: #fff;
  margin: 0;
  font-size: 14px;
  overflow: hidden;
}

.member_list .ul .li {
  width: 25%;
  display: block;
  float: left;
  position: relative;
  text-align: center;
  padding-bottom: 10px;
}

.member_list .ul .li .a {
  display: block;
  max-width: 80px;
  margin: 0 auto;
}

.member_list .ul .li .a image{
  display: block;
  width: 25.6px;
  height: 25.6px;
  margin: 3.2px auto;
}

.member_list .ul .li .a .span {
  font-size: 12px;
  margin-top: 5px;
  display: block;
  padding: 0 5px;
  overflow: hidden;
  white-space: nowrap;
}

.member_menu_list {
  margin: 0;
  padding: 0 10px;
  background: #fff;
}

.member_menu_list .ul {
  width: 100%;
}

.member_menu_list .ul .li {
  position: relative;
  line-height: 25px;
  padding: 10px 0px;
  border-bottom: 1px solid #eee;
}

.member_menu_list .ul .li image {
  width: 20px;
  height: 20px;
  vertical-align: top;
  margin-right: 7px;
  position: relative;
  top: 2px;
}

.member_menu_list .ul .li .span.menu-title {
  font-size: 14px;
  color: #333;
}

.member_menu_list .ul .li:after{
  position: absolute;
  content: '';
  display: inline-block;
  width: 9px;
  height: 9px;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 3px;
  border-top: 1px solid #999;
  border-right: 1px solid #999;
  transform: rotate(45deg);
  -o-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}
