.member_foot {
  width: 100%;
  height: 20px;
  background-size: 100% 20px;
  margin: 0 !important;
  padding: 0 !important;
  display: flex;
}

.info-content-top {
  width: 90%;
  margin: 0 5%;
  text-align: center;
  font-size: 12px;
  color: #fff;
  line-height: 20px;
}

.info-content-bottom {
  width: 90%;
  margin: 0 5%;
  text-align: center;
  font-size: 12px;
  color: #fff;
  line-height: 20px;
}

.info-content-bottom view{
  font-size: 20px;
  color: yellow;
  text-align: center;
  line-height: 30px;
}

.info-content-bottom view .level {
  padding: 0 5px;
  border-radius: 3px;
  background: none;
  color: #fff;
  font-size: 13px;
  letter-spacing:3px;

 
}
button::after{
 border:none;
}

.info-content-top text {
  width: 30%;
  height: 20px;
  line-height: 20px;
  text-align: center;
  display: inline-block;
  font-size: 13px;
}

.info-content-bottom text {
  display: inline-block;
  font-size: 20px;
  color: yellow;
  text-align: center;
  width: 30%;
  height: 30px;
  line-height: 30px;
}

.control {
  margin-top: 0 !important;
  padding: 5px 8px 0px 8px;
  overflow: hidden;
  border-bottom: 1px solid #e6e3e3;
  background: #fff;
}

.control_l_content {
  float: left;
  line-height: 40px;
  font-size: 14px;
  margin-left: 5px;
  font-weight: nomarl;
}

.control_l_content.selected {
  color: #000000;
}

.control_all_order {
  overflow: hidden;
  display: block;
  float: right;
  line-height: 40px;
  position: relative;
  color: #999;
  font-size: 14px;
}

.control_all_order:after{
  content: '';
  display: inline-block;
  width: 7px;
  height: 7px;
  margin-right: 5px;
  border-top: 1px solid #999;
  border-right: 1px solid #999;
  transform: rotate(45deg);
  -o-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}

.member-nav {
  margin: 0 0 0 0;
  border: 0;
  padding: 0;
}

.member-nav_item {
  position: relative;
  float: left;
  width: 20%;
  text-align: center;
  margin-bottom: 0px;
  background: #fff;
  padding: 10px 0;
}

.navigator_a{
  height: 100%;
}

.member-nav_item image {
  width: 25px;
  height: 25px;
}

.member-nav_item text {
  font-size: 12px;
  margin-top: 6px;
  display: inline-block;
  width: 100%;
}

.member_middle_spac {
  height: 10px;
  width: 100%;
  background-color: #fff;
  clear:both;
}

.member_list {
  list-style-type: none;
  margin-bottom: 10px;
  background: none;
  background-color: #fff;
  padding: 15px 0 5px 0;
  font-size: 14px;
  overflow: hidden;
}

